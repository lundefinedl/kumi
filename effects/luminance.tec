include common.tec;

technique luminance_map {
  vertex_shader {
      file effects/luminance.fx;
      entry_point quad_vs_main;
  };
  
  pixel_shader {
      file effects/luminance.fx;
      entry_point luminance_map_ps_main;
      params [
          texture2d rt_composite system;
      ];
  };
  
  geometry = fs_quad_postex;
  
  rasterizer_desc = NoCulling;
  depth_stencil_desc = DepthDisable;
  blend_desc = BlendDisable;
};

