include common.tec;

technique particles {
    
    vertex_shader {
        file effects/particles.hlsl;
        entry_point vs_main;
    };

    geometry_shader {
        file effects/particles.hlsl;
        entry_point gs_main;
    };

    pixel_shader {
        file effects/particles.hlsl;
        entry_point ps_main;
    };
    
    rasterizer_desc = NoCulling;
    depth_stencil_desc = DepthDisable;
    blend_desc = BlendOneOne;
};
