include common.tec;

technique hblur {
    compute_shader {
        file blur.hlsl;
        entry_point boxBlurX;
    };
};

technique vblur {
    compute_shader {
        file blur.hlsl;
        entry_point boxBlurY;
    };
};

technique copy_uav inherits quad_base {
    pixel_shader {
        file blur.hlsl;
        entry_point ps_copy_uav;
    };
};

