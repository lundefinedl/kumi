technique diffuse {
	
	vertex_shader {
		file effects/diffuse.fx;
		params [
			float4x4 view system;
			float4x4 proj system;
			float4x4 world mesh;
		];
	};
	
	pixel_shader {
		file effects/diffuse.fx;
		params [
			float4 Diffuse material;
		];
	};
	
	rasterizer_desc {
		cull_mode = back;
	};
	
	depth_stencil_desc {
		depth_enable = true;
	};
	
	blend_desc {
		render_target[0] = {
			blend_enable = false;
		};
	};
};

