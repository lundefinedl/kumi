rasterizer_desc BackfaceCulling { 
  cull_mode = back; 
};

rasterizer_desc NoCulling { 
  cull_mode = back; 
};

depth_stencil_desc DepthEnable { 
  depth_enable = true; 
};

depth_stencil_desc DepthDisable { 
  depth_enable = false; 
};

blend_desc BlendDisable {
  render_target[0] = {
    blend_enable = false;
  };
};

blend_desc BlendOneOne {
  render_target[0] = {
    blend_enable = true;
    src_blend = one;
    dest_blend = one;
  };
};
