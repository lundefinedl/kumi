technique gwen {
	
	vertex_shader {
		file effects/gwen.fx;
		entry_point vs_color_main;
	};
	
	pixel_shader {
		entry_point ps_color_main;
		file effects/gwen.fx;
	};
	
	blend_desc {
		render_target[0] = {
			blend_enable = true;
			src_blend = src_alpha;
			dest_blend = inv_src_alpha;
			blend_op = add;
			src_blend_alpha = one;
			dest_blend_alpha = zero;
			blend_op_alpha = add;
		};
	};
	
	rasterizer_desc {
	};
	
	depth_stencil_desc {
		depth_enable = false;
	};
};
